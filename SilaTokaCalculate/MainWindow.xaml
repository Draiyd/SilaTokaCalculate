<Window x:Class="SilaTokaCalculate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Калькулятор закона Ома" Height="200" Width="350">
    <Grid Margin="10">
        <StackPanel>
            
            <TextBlock Text="Данные из файла:" Margin="0,0,0,5" FontWeight="Bold"/>
            <TextBox x:Name="txtFileData" Height="60" TextWrapping="Wrap" IsReadOnly="True" Margin="0,0,0,10"/>

            
            <Button Content="Вычислить силу тока" Click="CalculateCurrent_Click" Height="30" Margin="0,0,0,10"/>

            
            <TextBlock x:Name="txtResult" FontWeight="Bold" TextWrapping="Wrap"/>
        </StackPanel>
    </Grid>
</Window>